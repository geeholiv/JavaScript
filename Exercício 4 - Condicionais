    <script>
        const senhaCorreta = "12345";
        let senhaDigitada = prompt("Digite a senha:");

        if (senhaDigitada === senhaCorreta) {
            document.Write("Acesso permitido!");
        } else {
            document.Write("Senha incorreta. Acesso negado!");
        }
    </script>
