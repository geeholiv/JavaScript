<body>
    <script>
        let num = parseInt(prompt("Digite um número:"));
        
        if (num >= 10 && num <= 20) {
            document.write("O número está na faixa de 10 a 20.");
        } else {
            document.write("Fora da faixa.");
        }
    </script>
    
</body>
