        let valorhora = Number(prompt("Valor da hora: "));
        let horastrabalhadas = Number(prompt("Horas trabalhadas no mês: "));

        let multiplicacao = horastrabalhadas * valorhora;

        alert("Valor da hora: " + valorhora + "\nHoras trabalhadas no mês: " + horastrabalhadas + "\nSalário: " + multiplicacao);
        

