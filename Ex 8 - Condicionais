<body>
    <script>
        let numero = parseInt(prompt("Digite um número:"));
        if (numero % 5 === 0) {
            document.write("É múltiplo de 5.");
        } else {
            document.write("Não é múltiplo de 5.");
        }
    </script>
    
</body>
